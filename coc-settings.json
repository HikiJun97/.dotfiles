{
  "suggest.noselect": true,
  "tsserver.log": "verbose",
  "languageserver": {
    "dockercompose": {
      "command": "docker-compose-langserver",
      "args": ["--stdio"],
      "filetypes": ["dockercompose"],
      "rootPatterns": [".git", ".env", "docker-compose.yml", "compose.yml"]
